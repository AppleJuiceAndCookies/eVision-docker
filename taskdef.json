{
    "containerDefinitions": [ 
       { 
          "essential": true,
          "image": "$CONTAINER_IMAGE",
          "name": "$REPOSITORY_URI/$IMAGE_REPO_NAME",
          "portMappings": [ 
             { 
                "containerPort": "$CONTAINER_PORT",
                "hostPort": "$CONTAINER_PORT",
                "protocol": "tcp"
             }
          ]
       }
    ],
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::$ACCOUNT_ID:role/$CONTAINER_ROLE_ARN",
    "memory": "512",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [ 
        "FARGATE" 
     ]
 }